#inlcude<stdio.h>

int main()
{
 printf("\tNumberFound");
    int n,a,count,d1,m1,y1;
    do{
        printf("Input your number: ");
        scanf("%d", &n);
    }while(n<=0);

    printf("Input number that you search: ");
    scanf("%d",&a);
    printf("\n Is it found?");

    count=0;
    while(n!=0){
        if(n%10==a){
            count++;
            break;
        }

        n=n/10;
    }
    if(count>0){
        printf("\n Yes");
    }else{
    printf("\n No");

return 0;
}
