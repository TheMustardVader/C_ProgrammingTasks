#include<stdio.h>

int main()
{

printf("\tBackSide");
    int n,m;
    do{
        printf("Input your  N number: ");
        scanf("%d",&n);

    }while(n<=0);
      m=0;
        while(n>0){
            m=n%10+m*10;
            n=n/10;
        }
        printf("Your opposite number = %d",m);
return 0;
}
