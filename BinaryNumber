#include <stdio.h>

int main(){

printf("\tBinaryNumber");
    int i, n, count;
    printf("\t BinaryNumber");
    do{
        printf("\n Input your number:");
        scanf("%d",&n);
     }while(n<0);
     i=1;
     count=0;
     while(i<=n){
        if(n%i==0){
            printf("\n%d",i);
            if(count%2!=0){
                count++;}
                }i++;
        }
     printf("\n count = %d",count);
     
return 0;
}
