#include<stdio.h>

int main()
{
 printf("\tMaxOfN");
    int a,max,i,n;
    do{
    printf("\n Input number n:");
    scanf("%d",&n);
    }
    while(n<=0);
    printf("\n Input a:");
    scanf("%d", &a);
    max=a;
    for(i=1;i<n;i++){
        printf("\n Input a:");
        scanf("%d", &a);
        if(a>max)max=a;
    }
    printf("\n Max = %d",max);
    return 0;
}
