#include<stdio.h>

int main(){

printf("\tAscii table");
    int count,i;
    count=0;
    i=26;
    while(count < 256){
        printf(" %d --> %c  \n",count,count);
        count++;
        if(count == i){
        i+=25;
        printf("Press enter ==>");
        getchar();
        }
    }

return 0;
}
