#include <stdio.h>

int main(){ 

int i,n,count;
printf("\tDelAndCount");
   do{
    printf("\n Input your number: ");
    scanf("%d", &n);
   }while(n<=0);
   i=1;
   count=0;
   while(i<=n){
    if(n%i==0){
        printf("\n%d", i);
        count++;
    }
    i++;
   }
   printf("\n count = %d",count);

return 0;
}
