#include<stdio.h>

int main(){
printf("\t Count 1 in binary N");
   int count,n;

   do{
    printf("\n Input your number: ");
    scanf("%d", &n);
   }while(n<=0);

   count=0;

   do{
        if(n%2==1){
        count++;
        }
        n=n/2;
    }
        while(n!=0);
   printf("\n count = %d",count);
return 0;
}
